@using lab2.ViewModel
@model List<CoursDeptcs>


<h1>courses</h1>
<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Degree</th>
        <th>MinDegree</th>
        <th>Hours</th>
        <th>Department</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Degree</td>
            <td>@item.MinDegree</td>
            <td>@item.Hours</td>
         
            <td>@item.DepartmentName</td>
            

        </tr>
    }
</table>



